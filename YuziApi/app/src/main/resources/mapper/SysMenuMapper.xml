<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.naraci.app.BaseServer.mapper.SysMenuMapper">

    <resultMap id="BaseResultMap" type="com.naraci.app.BaseServer.domain.SysMenu">
            <id property="id" column="id" jdbcType="VARCHAR"/>
            <result property="createTime" column="creat_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
            <result property="displayName" column="display_name" jdbcType="VARCHAR"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="icon" column="icon" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,creat_time,update_time,
        display_name,name,icon
    </sql>
    <select id="findMenuNameOrPath" resultType="com.naraci.app.BaseServer.domain.SysMenu">
        select * from sys_menu where display_name = #{displayName} OR name = #{name}
    </select>
</mapper>
